{
  "name": "boryx",
  "version": "0.0.1",
  "description": "A node framework for educational purposes",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build:clean": "rm -rf dist && mkdir dist",
    "build": "npm run build:clean && tsc",
    "lint:format": "prettier --write .",
    "prepublishOnly": "npm run build && lint:format",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:format"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aoculi/boryx.git"
  },
  "keywords": [
    "node",
    "framework"
  ],
  "author": "Alexis Oculi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aoculi/boryx/issues"
  },
  "homepage": "https://github.com/aoculi/boryx#readme",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.18",
    "@types/supertest": "^2.0.11",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
}
